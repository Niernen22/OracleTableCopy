---------------------------------------------------------------------------------------FORRAS_TABLA_1
DROP TABLE FORRAS_TABLA_1;

CREATE TABLE FORRAS_TABLA_1
(
       ID NUMBER,
       NAME VARCHAR2(20),
       PARTITION_DATE DATE
);

INSERT INTO FORRAS_TABLA_1 VALUES (1, 'PÉLDA1', TO_DATE('2023-02-22', 'yyyy-mm-dd'));
INSERT INTO FORRAS_TABLA_1 VALUES (2, 'PÉLDA2', TO_DATE('2023-02-23', 'yyyy-mm-dd'));
INSERT INTO FORRAS_TABLA_1 VALUES (3, 'PÉLDA3', TO_DATE('2023-02-24', 'yyyy-mm-dd'));
INSERT INTO FORRAS_TABLA_1 VALUES (4, 'PÉLDA4', TO_DATE('2023-02-25', 'yyyy-mm-dd'));

SELECT * FROM FORRAS_TABLA_1;
---------------------------------------------------------------------------------------FORRAS_TABLA_2
DROP TABLE FORRAS_TABLA_2;

CREATE TABLE FORRAS_TABLA_2 
(
       ID NUMBER,
       NAME VARCHAR2(20),
       PARTITION_DATE DATE
);

ALTER TABLE FORRAS_TABLA_2
MODIFY PARTITION BY LIST (PARTITION_DATE) (
    PARTITION P_230222 VALUES (TO_DATE('2023-02-22', 'YYYY-MM-DD')),
    PARTITION P_230223 VALUES (TO_DATE('2023-02-23', 'YYYY-MM-DD')),
    PARTITION P_230224 VALUES (TO_DATE('2023-02-24', 'YYYY-MM-DD')),
    PARTITION P_230225 VALUES (TO_DATE('2023-02-25', 'YYYY-MM-DD'))
);

INSERT INTO FORRAS_TABLA_2 VALUES (1, 'PÉLDA1', TO_DATE('2023-02-22', 'yyyy-mm-dd'));
