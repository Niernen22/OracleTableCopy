---------------------------------------------------------------------------------------FORRAS_TABLA_3
DROP TABLE FORRAS_TABLA_3;

CREATE TABLE FORRAS_TABLA_3 (
    ID NUMBER,
    NAME VARCHAR2(20),
    PARTITION_DATE DATE
)
PARTITION BY RANGE (PARTITION_DATE)
(
    PARTITION P_230222 VALUES LESS THAN (TO_DATE('2023-02-23', 'YYYY-MM-DD')),
    PARTITION P_230223 VALUES LESS THAN (TO_DATE('2023-02-24', 'YYYY-MM-DD')),
    PARTITION P_230224 VALUES LESS THAN (TO_DATE('2023-02-25', 'YYYY-MM-DD')),
    PARTITION P_230225 VALUES LESS THAN (TO_DATE('2023-02-26', 'YYYY-MM-DD'))
);

INSERT INTO FORRAS_TABLA_3 VALUES (1, 'PÉLDA1', TO_DATE('2023-02-22', 'yyyy-mm-dd'));
